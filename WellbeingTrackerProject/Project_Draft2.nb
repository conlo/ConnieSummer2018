Cell[BoxData[
 RowBox[{
  RowBox[{"countPositive", "[", " ", "text_String", " ", "]"}], " ", ":=", 
  " ", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"sentences", ",", " ", "words", ",", " ", "positivePhrases"}], 
     "}"}], ",", "\n", "\n", "\t", 
    RowBox[{
     RowBox[{"sentences", " ", "=", " ", 
      RowBox[{"TextCases", "[", 
       RowBox[{"text", ",", " ", "\"\<Sentence\>\""}], "]"}]}], ";", "\n", 
     "\t", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"i", " ", "=", " ", "0"}], ",", " ", 
       RowBox[{"i", " ", "\[LessEqual]", " ", 
        RowBox[{"Length", "[", "sentences", "]"}]}], ",", " ", 
       RowBox[{"i", "++"}], ",", " ", 
       RowBox[{
        RowBox[{"Classify", "[", 
         RowBox[{"\"\<Sentiment\>\"", ",", 
          RowBox[{"sentences", "[", 
           RowBox[{"[", "i", "]"}], "]"}], ",", " ", 
          "\"\<Probabilities\>\""}], "]"}], ";", "\n", "\t", "\n", "\t", 
        RowBox[{"positivePhrases", "=", 
         RowBox[{"{", "}"}]}], ";", "\n", "\t\t", 
        RowBox[{
         RowBox[{
          RowBox[{"If", "[", 
           RowBox[{
            RowBox[{
            "\"\<Sentiment\>\"", " ", "\[Equal]", "\"\<Positive\>\""}], ",", 
            " ", 
            RowBox[{"AppendTo", "[", 
             RowBox[{"positivePhrases", ",", " ", "sentence"}], "]"}], ",", 
            " ", "Nothing"}], "]"}], "\n", "\t\t", "\n", "\t\t\t", "get", " ",
           "list", " ", "of", " ", "activities", " ", "and", " ", 
          "probability", " ", "counts", " ", "from", " ", "sentence", "\n", 
          "\t\t\t", "\n", "\t\t\t\t", "words"}], " ", "=", " ", 
         RowBox[{"TextCases", "[", 
          RowBox[{"text", ",", " ", "\"\<Word\>\""}], "]"}]}], ";", "\n", 
        "\t\t\t\t", 
        RowBox[{"positiveWords", " ", "=", " ", 
         RowBox[{
          RowBox[{
           RowBox[{"Map", "[", 
            RowBox[{"PartOfSpeech", ",", "words"}], "]"}], "[", 
           RowBox[{"[", "i", "]"}], "]"}], "[", 
          RowBox[{"[", "1", "]"}], "]"}]}], ";", "\n", "\t\t\t\t", "\n", "\t", 
        RowBox[{"WHat", " ", "to", " ", 
         RowBox[{"return", ":", " ", 
